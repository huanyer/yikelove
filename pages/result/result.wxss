.res-title {
  color: #fff;
  display: block;
  font-family: STYuanti-TC-Bold;
  font-size: 32rpx;
  padding: 10px 0 0 8%;
}

.res-top {
  color: #fff;
  display: block;
  padding-left: 20px;
  font-family: STYuanti-TC-Bold;
  font-size: 72rpx;
}

.res-chart{
  background:#fff;
  /* width: 320px; */
  width: 90%;
  margin: 18% auto 30rpx;
  border-radius: 20rpx;
  padding-top: 20rpx;
}
.res-info {
  width: 90%;
  margin: auto;
  display: flex;
  align-items:center;
  border-bottom: 2rpx dashed #ececec;
}

.res-info image {
  width: 75px;
  margin-right: 10px;
}

.content {
  flex: 1;

}

.res-score {
  display: block;
  font-size: 26px;
  color: #fe9710;
}

.res-ranking {
  display: block;
  font-size: 16px;
}

.yk_btn.save-pic {
  color: #000;
  width: 50%;
  background-image: linear-gradient(-180deg, #ffef5e 0%, #ffdb30 100%);
}

.evaluate {
  padding: 0 40rpx;
  position: relative;
  background: #fff;
}

.evaluate-title {
  font-size: 36rpx;
  position: absolute;
  top: -60rpx;
  left: 0;
  width: 100%;
  text-align: center;
}

.evaluate-info {
  font-size: 28rpx;
  margin-top: 20rpx;
}

.see-more {
  background-image: linear-gradient(-180deg, rgba(255, 255, 255, 0.00) 0%, #fff 75%);
  position: absolute;
  width: 100%;
  left: 0;
  height: 100rpx;
  bottom: 60rpx;
}

.see-more-btn {
  background-image: linear-gradient(-120deg, #fc775d 0%, #ff9f00 100%);
  width: 200rpx;
  color: #fff;
  height: 60rpx;
  line-height: 55rpx;
  background-color: #fff;
  font-size: 32rpx;
}

.tab-title {
  background-color: #fff;
  font-family: PingFangSC-Semibold;
  font-size: 36rpx;
  color: #060606;
  padding: 30rpx 0 20rpx;
  text-align: center;
}
.more-test-btn{
  background: #FFFFFF;
  box-shadow: 0 4rpx 16rpx 0 rgba(0,0,0,0.12);
  border-radius: 26rpx;
  font-family: STYuanti-TC-Bold;
  font-size: 40rpx;
  color: #FF9F00;
  width: 90%;
}

@import '../components/testList/TestList.wxss';